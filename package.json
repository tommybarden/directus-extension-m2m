{
	"name": "directus-extension-m2m",
	"description": "Directus extension for exporting/importing data structure between different installs",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-bundle"
	],
	"type": "module",
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "module",
				"name": "Export",
				"source": "src/export.js"
			},
			{
				"type": "hook",
				"name": "Import",
				"source": "src/import.js"
			}
		],
		"host": "^10.1.9"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.1.9",
		"vue": "^3.3.4"
	},
	"dependencies": {
		"@directus/composables": "^10.1.1",
		"lodash": "^4.17.21"
	}
}
